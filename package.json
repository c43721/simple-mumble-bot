{
  "name": "simple-mumble-bot",
  "version": "0.0.1",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "build": "tsc",
    "compile-proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=proto/ --ts_proto_opt=esModuleInterop=true --ts_proto_opt=exportCommonSymbols=false --ts_proto_opt=outputTypeRegistry=true Mumble.proto"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "rxjs": "7.5.5",
    "ts-proto": "^1.112.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  }
}
